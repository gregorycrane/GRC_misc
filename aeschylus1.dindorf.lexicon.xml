    <def xml:lang="lat">Baccha</def>
    
    <cit>
      <bibl>Eum. 25</bibl>
      <quote xml:lang="grc">ἐξ οὔτε Βάκχαις ἐστρατήγησεν θεός</quote>
    </cit>
  </sense>
